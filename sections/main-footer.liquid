{{ 'main-footer.css' | asset_url | stylesheet_tag }}

<footer class="main-footer">
  <div class="main-footer__container">
    <div class="main-footer__top">
      {% if section.settings.footer_logo != blank %}
        <div class="main-footer__logo" data-aos="fade-up" data-aos-offset="0" data-aos-duration="800" data-aos-easing="ease-out" style="--aos-translate-y: 5px;">
          <a href="/">
            <img src="{{ section.settings.footer_logo | image_url }}" alt="{{ section.settings.footer_logo.alt }}">
          </a>
        </div>
      {% endif %}
      <div class="main-footer__links">
        {% for block in section.blocks %}
          {% if block.type == 'links_block' %}
            <div class="main-footer__link main-footer__link--{{ forloop.index }}">
              {% if block.settings.title != blank %}
                <p class="main-footer__link-title color-black" data-aos="fade-up" data-aos-offset="0" data-aos-duration="800" data-aos-easing="ease-out" style="--aos-translate-y: 5px">{{ block.settings.title }}</p>
              {% endif %}
              {% if block.settings.menu != blank %}
                <ul class="main-footer__link-list">
                  {% for link in block.settings.menu.links %}
                    <li class="main-footer__link-item">
                      <a href="{{ link.url }}" class="main-footer__link-url color-light-black2" data-aos="fade-up" data-aos-offset="0" data-aos-duration="800" data-aos-easing="ease-out" style="--aos-translate-y: 5px;">{{ link.title | escape }}</a>
                    </li>
                  {% endfor %}
                </ul>
              {% endif %}
            </div>
          {% endif %}
        {% endfor %}
      </div>
    </div>
    <div class="main-footer__bottom">
      <div class="main-footer__social">
        <div class="main-footer__insta">
          {% if section.settings.instagram_link != blank %}
            <a href="{{ section.settings.instagram_link }}" data-aos="fade-up" data-aos-offset="0" data-aos-duration="800" data-aos-easing="ease-out" style="--aos-translate-y: 5px;">
              <svg width="49" height="38" viewBox="0 0 49 38" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                <rect x="0.0898438" y="0.765381" width="48.6778" height="36.5084" fill="url(#pattern0_15030_784)"/>
                <defs>
                <pattern id="pattern0_15030_784" patternContentUnits="objectBoundingBox" width="1" height="1">
                <use xlink:href="#image0_15030_784" transform="scale(0.0015625 0.00208333)"/>
                </pattern>
                <image id="image0_15030_784" width="640" height="480" preserveAspectRatio="none" xlink:href="data:image/png;base64,"/>
                </defs>
              </svg>
            </a>
          {% endif %}
        </div>
        <div class="main-footer__wholesale">
          {% if section.settings.wholesale_text != blank %}
            <a href="{{ section.settings.wholesale_url }}" class="main-footer__wholesale-link color-light-black2 text-center" data-aos="fade-up" data-aos-offset="0" data-aos-duration="800" data-aos-easing="ease-out" style="--aos-translate-y: 5px;">{{ section.settings.wholesale_text }}</a>
          {% endif %}
        </div>
      </div>
      <div class="main-footer__policies">
        <div class="main-footer__policies-menu">
          {% for block in section.blocks %}
            {% if block.type == 'policies_block' %}
              <ul class="main-footer__policies-list">
                {% for link in block.settings.policies_menu.links %}
                  <li class="main-footer__policies-item" data-aos="fade-up" data-aos-offset="0" data-aos-duration="800" data-aos-easing="ease-out" style="--aos-translate-y: 5px;">
                    <a href="{{ link.url }}" class="main-footer__policies-link color-light-black2">{{ link.title | escape }}</a>
                  </li>
                {% endfor %}
              </ul>
            {% endif %}
          {% endfor %}
        </div>
        <div class="main-footer__copy" data-aos="fade-up" data-aos-offset="0" data-aos-duration="800" data-aos-easing="ease-out" style="--aos-translate-y: 5px;">
          <span class="main-footer__copy-year color-light-black2">
            Botnia Â©{{ 'now' | date: '%Y' }}
          </span>
          {% if section.settings.copy_right != blank %}
            <span class="main-footer__copy-dev color-light-black2">
              {{ section.settings.copy_right | remove: '<p>' | remove: '</p>'   }}
            </span>
          {% endif %}
        </div>
      </div>
    </div>
  </div>
</footer>

{% schema %}
  {
    "name": "Main Footer",
    "limit": 1,
    "enabled_on": {
      "groups": ["footer"]
    },
    "settings": [
      {
        "type": "image_picker",
        "id": "footer_logo",
        "label": "Footer Logo"
      },
      {
        "type": "url",
        "id": "instagram_link",
        "label": "Instagram Link"
      },
      {
        "type": "text",
        "id": "wholesale_text",
        "label": "Wholesale Text"
      },
      {
        "type": "url",
        "id": "wholesale_url",
        "label": "Wholesale URL"
      },
      {
        "type": "richtext",
        "id": "copy_right",
        "label": "Copyright Developer"
      }
    ],
    "blocks": [
      {
        "type": "links_block",
        "name": "Links Block",
        "limit": 4,
        "settings": [
          {
            "type": "text",
            "id": "title",
            "label": "Title"
          },
          {
            "type": "link_list",
            "id": "menu",
            "label": "Menu"
          }
        ]
      },
      {
        "type": "policies_block",
        "name": "Policies",
        "settings": [
          {
            "type": "link_list",
            "id": "policies_menu",
            "label": "Policies Menu"
          }
        ]
      }
    ],
    "presets": [
      {
        "name": "Main Footer",
        "category": "Footer",
        "blocks": []
      }
    ]
  }
{% endschema %}
  